  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7CCD7C"
      }
    },

    {
      "name": "constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#FA8072"
      }
    },

    {
      "name": "entity",
      "scope": ["entity"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Overwrite to keep `entity` default",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    // Workaround for SCSS syntax's handling of percentages in first-level @-rule definitions,
    // ie: `12` in:
    // ```
    // @keyframes foo {
    //   12.34% { /*...*/ }
    // }
    // ```
    {
      "name": "Digits in @-rule name (see comment above)",
      "scope": ["meta.at-rule.keyframes.scss"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Overwrite to set brackets back to intended color",
      "scope": ["meta.at-rule.keyframes.scss punctuation.section.keyframes"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "HTML tags",
      "scope": ["entity.name.tag", "entity.name.tag support.class.component"],
      "settings": {
        "foreground": "#EEDC82"
      }
    },

    {
      "name": "keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#808080"
      }
    },

    {
      "name": "`type` in `import type { foo } from 'bar'`",
      "scope": ["keyword.control.type.ts", "keyword.control.type.tsx"],
      "settings": {
        "foreground": "#BCB8A4"
      }
    },

    {
      "scope": ["keyword.operator.accessor"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "CSS - tag-name",
      "scope": [
        "meta.selector.css",
        "meta.selector.css entity.name.tag",
        "entity.name.tag.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    // TODO: this doesn't work
    // {
    //   "name": "Sass - `cursor` and `content` properties are incorrectly identified as tag names",
    //   "scope": [
    //     "entity.name.tag.css meta.property-list.scss meta.property-list.scss source.css.scss"
    //   ],
    //   "settings": {
    //     "foreground": "#ff0000"
    //   }
    // },

    {
      "name": "CSS - property name",
      "scope": ["meta.property-list.css", "support.type.property-name.css"],
      "settings": {
        "foreground": "#EEDC82"
      }
    },

    {
      "name": "CSS - id",
      "scope": [
        "meta.selector.css entity.other.attribute-name.id",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    {
      "name": "CSS - Property Value: Constant",
      "scope": [
        "constant.numeric.css",
        "constant.other.color.rgb-value.hex",
        "keyword.operator.css",
        "keyword.operator.arithmetic.css",
        "meta.property-value",
        "meta.property-value support.constant.property-value"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "CSS - `!important`",
      "scope": ["keyword.other.important"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D21932"
      }
    },

    {
      "name": "CSS/SCSS - comma, colon, semi-colon, curly brace, parenthesis",
      "scope": [
        "meta.property-value punctuation.separator.list.comma",
        "meta.property-list.scss meta.at-rule.include.scss punctuation.definition.parameters",
        "punctuation.section.function",
        "punctuation.section.property-list",
        "punctuation.separator.delimiter",
        "punctuation.separator.key-value",
        "punctuation.terminator.rule"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "name": "SCSS - comma in @include mixin value",
      "scope": ["meta.at-rule.include.scss"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "Sass - bracket, parenthesis",
      "scope": [
        "punctuation.definition.attribute-selector",
        "punctuation.definition.media-query"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "CSS/SCSS `>` in selector",
      "scope": ["keyword.operator.combinator.css", "meta.property-list.scss"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "CSS - pseudo element",
