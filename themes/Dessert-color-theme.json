        "foreground": "#D2A8A1"
      }
    },

    {
      "name": "invalid.illegal",
      "scope": ["invalid.illegal"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE799F"
      }
    },

    {
      "name": "TS `@` decorator",
      "scope": [
        "meta.decorator meta.function-call entity.name.function",
        "punctuation.decorator.tsx"
      ],
      "settings": {
        "foreground": "#8A998A"
      }
    },

    {
      "name": "meta.object-literal.key: all levels",
