  // https://code.visualstudio.com/api/language-extensions/semantic-highlight-guide
  // "semanticHighlighting": true,

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#526652"
      }
    },

    {
      "name": "constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#FA8072"
      }
    },

    {
      "name": "entity",
      "scope": ["entity"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Overwrite to keep `entity` default",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "meta.function-call entity.name.function (JS/TS)",
      "scope": ["meta.function-call entity.name.function"],
      "settings": {
        "foreground": "#eeb681"
      }
    },

    {
      "scope": [
        // `{%`, `%}`, `{{`, and `}}` (Nunjucks)
        "entity.tag.tagbraces.nunjucks"
      ],
      "settings": {
        "foreground": "#CE93D8"
      }
    },

    // Workaround for SCSS syntax's handling of percentages in first-level @-rule definitions,
    // ie: `12` in:
    // ```
    // @keyframes foo {
    //   12.34% { /*...*/ }
    // }
    // ```
    {
      "name": "Digits in @-rule name (see comment above)",
      "scope": ["meta.at-rule.keyframes.scss"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Overwrite to set brackets back to intended color",
      "scope": ["meta.at-rule.keyframes.scss punctuation.section.keyframes"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "HTML tags",
      "scope": ["entity.name.tag", "entity.name.tag support.class.component"],
      "settings": {
        "foreground": "#EEDC82"
      }
    },

    {
      "name": "keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#808080"
      }
    },

    {
      "scope": [
        // `title` in `{{ title }}` (Nunjucks)
        "keyword.control.filter.nunjucks"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "Conditional Control keywords (JS/TS)",
      "scope": [
        // `if`, `else`, `else if`
        "keyword.control.conditional"
      ],
      "settings": {
        "foreground": "#BCB8A4"
      }
    },

    {
      "name": "Switch Control keywords (JS/TS)",
      "scope": [
        // `switch`, `case`
        "keyword.control.switch"
      ],
      "settings": {
        "foreground": "#76EEC6"
      }
    },

    {
      "name": "Control keywords (JS/TS)",
      "scope": [
        // `for`, `break`, `while`
        "keyword.control.loop",

        // `try`, `catch`
        "keyword.control.trycatch"
      ],
      "settings": {
        "foreground": "#BCB8A4"
      }
    },

    {
      "name": "CSS/SCSS at-rule (ie. `@media`)",
      "scope": ["keyword.control.at-rule"],
      "settings": {
        "foreground": "#EE799F"
      }
    },

    {
      "name": "Sass `@extend` and `@include`",
      "scope": [
        "keyword.control.at-rule.extend",
        "keyword.control.at-rule.include"
      ],
      "settings": {
        "foreground": "#BCB8A4"
      }
    },

    // {
    //   "name": "import statement keywords",
    //   "scope": [
    //     // `@import` (Sass)
    //     "keyword.control.at-rule.import.scss",
    //     // `import` (JS/TS)
    //     "keyword.control.import",
    //     // `as` (JS/TS)
    //     "keyword.control.as",
    //     // `from` (JS/TS)
    //     "keyword.control.from"
    //   ],
    //   "settings": {
    //     "foreground": "#808080"
    //   }
    // },

    {
      "name": "`return`, `await`, and `export` keywords (JS/TS)",
      "scope": [
        // `return` and `await`
        "keyword.control.flow",
        // `export`
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#CE93D8"
      }
    },

    {
      "name": "`type` in `import type { foo } from 'bar'` (JS/TS)",
      "scope": [
        // `type` (JS/TS)
        "keyword.control.type"
      ],
      "settings": {
        "foreground": "#a4bda4"
      }
    },

    // {
    //   "name": "operator keyword",
    //   "scope": ["keyword.operator"],
    //   "settings": {
    //     "foreground": "#EE799F"
    //     // "foreground": "#F00"
    //   }
    // },

    {
      "name": "operator keywords",
      "scope": [
        // `=` (JS/TS)
        "keyword.operator.assignment",
        // `&&`, `!`, `||` (JS/TS)
        "keyword.operator.logical",
        // `>=`, `>`, etc (JS/TS)
        "keyword.operator.relational",
        // `?`, `:` (JS/TS)
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#EE799F"
      }
    },

    {
      "scope": ["keyword.operator.accessor"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "scope": [
        // `typeof` in JS/TS
        "keyword.operator.expression.typeof"
      ],
      "settings": {
        "foreground": "#EE799F"
      }
    },
    {
      "scope": [
        // `delete`, `void`, `typeof` in JS/TS
        "keyword.operator.expression"
      ],
      "settings": {
        "foreground": "#CE93D8"
      }
    },

    {
      "name": "`async` keywords (JS/TS)",
      "scope": ["storage.modifier.async"],
      "settings": {
        "foreground": "#BCB8A4"
      }
    },

    {
      "name": "CSS/SCSS - tag-name, `&`",
      "scope": [
        "meta.selector.css",
        "meta.selector.css entity.name.tag",
        "entity.name.tag.css",
        "entity.name.tag.reference.scss",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    // TODO: this doesn't work
    // {
    //   "name": "Sass - `cursor` and `content` properties are incorrectly identified as tag names",
    //   "scope": [
    //     "entity.name.tag.css meta.property-list.scss meta.property-list.scss source.css.scss"
    //   ],
    //   "settings": {
    //     "foreground": "#ff0000"
    //   }
    // },

    {
      "name": "CSS - property name",
      "scope": ["meta.property-list.css", "support.type.property-name.css"],
      "settings": {
        "foreground": "#EEDC82"
      }
    },

    {
      "name": "CSS - id",
      "scope": [
        "meta.selector.css entity.other.attribute-name.id",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    {
      "name": "CSS - Property Value: Constant",
      "scope": [
        "constant.numeric.css",
        "constant.other.color.rgb-value.hex",
        "keyword.operator.css",
        "keyword.operator.arithmetic.css",
        "meta.property-value",
        "meta.property-value support.constant.property-value"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "CSS - `!important`",
      "scope": ["keyword.other.important"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D21932"
      }
    },

    {
      "name": "CSS/SCSS - comma, colon, semi-colon, curly brace, parenthesis",
      "scope": [
        "meta.property-value punctuation.separator.list.comma",
        "meta.property-list.scss meta.at-rule.include.scss punctuation.definition.parameters",
        "punctuation.section.function",
        "punctuation.section.property-list",
        "punctuation.separator.delimiter",
        "punctuation.separator.key-value",
        "punctuation.terminator.rule"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "name": "SCSS - comma in @include mixin value",
      "scope": ["meta.at-rule.include.scss"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "Sass - bracket, parenthesis",
      "scope": [
        "punctuation.definition.attribute-selector",
        "punctuation.definition.media-query"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "CSS/SCSS `>` in selector",
      "scope": ["keyword.operator.combinator.css", "meta.property-list.scss"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "CSS - pseudo element",
      "scope": ["entity.other.attribute-name.pseudo-element.css"],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    {
      "scope": ["string"],
      "settings": {
        "foreground": "#FA8072"
      }
    },

    {
      "name": "Arguments in TS `@` decorator",
      "scope": ["meta.decorator.tsx string", "meta.decorator.tsx constant"],
      "settings": {
        "foreground": "#A5CD7C"
      }
    },

    {
      "name": "String in `import` statements",
      "scope": [
        // Go
        "string.quoted.double.go entity.name.import.go",
        // JS, TS
        "meta.import string.quoted",
        // SCSS
        "meta.at-rule.import.scss string.quoted.double.scss"
      ],
      "settings": {
        "foreground": "#f19898"
      }
    },
    {
      "name": "string.quoted.double.json",
      "scope": "string.quoted.double.json",
      "settings": {
        "foreground": "#EEDC82"
      }
    },
    {
      "scope": ["storage.type.variable string"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "scope": ["storage.type.templatetag string.unquoted.tag-string.nunjucks"],
      "settings": {
        "foreground": "#76EEC6"
      }
    },

    {
      "name": "invalid",
      "scope": ["invalid"],
      "settings": {
        // "foreground": "#EE799F"
        "foreground": "#D21932"
      }
    },
    {
      "name": "invalid.deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
