  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7CCD7D"
      }
    },

    {
      "name": "constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#FA8072"
      }
    },

    {
      "name": "entity",
      "scope": ["entity"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "HTML tags",
      "scope": ["entity.name.tag", "entity.name.tag support.class.component"],
      "settings": {
        "foreground": "#EEDC82"
      }
    },

    {
      "name": "keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#EE799F"
      }
    },
    {
      "name": "`import` and `from` (in JS)",
      "scope": ["keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "Unit (ie, in CSS value)",
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "scope": ["keyword.operator.accessor"],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "CSS - tag-name",
      "scope": [
        "meta.selector.css",
        "meta.selector.css entity.name.tag",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    {
      "name": "CSS - property name",
      "scope": ["meta.property-list.css", "support.type.property-name.css"],
      "settings": {
        "foreground": "#EEDC82"
      }
    },

    {
      "name": "CSS - id",
      "scope": [
        "meta.selector.css entity.other.attribute-name.id",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    // {
    //   "name": "CSS - property value",
    //   "scope": [

    //   ],
    //   "settings": {
    //     "foreground": "#87CEEB"
    //   }
    // },

    {
      "name": "CSS - Property Value: Constant",
      "scope": [
        "constant.numeric.css",
        // "constant.other.color.rgb-value.css",
        "constant.other.color.rgb-value.hex",
        "keyword.operator.css",
        // "meta.property-group support.constant.property-value.css",
        "meta.property-value",
        "meta.property-value support.constant.property-value"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "CSS - !important",
      "scope": ["keyword.other.important"],
      "settings": {
        "foreground": "#D21932"
      }
    },

    {
      "name": "CSS - colon, semi-colon, curly brace",
      "scope": [
        "punctuation.section.property-list",
        "punctuation.separator.key-value",
        "punctuation.terminator.rule"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "CSS >",
      "scope": ["keyword.operator.combinator.css"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },

    {
      "name": "CSS - pseudo element",
      "scope": ["entity.other.attribute-name.pseudo-element.css"],
      "settings": {
        "foreground": "#2B91AF"
      }
    },

    {
      "name": "storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#EE799F"
      }
    },

    {
      "name": "string",
      "scope": ["string"],
      "settings": {
        "foreground": "#FA8072"
      }
    },

    {
      "name": "support",
      "scope": ["support"],
      "settings": {
        "foreground": "#FFA54F"
      }
    },
    {
      "name": "CSS - Generic font name (ie. serif)",
      "scope": ["support.constant.font-name"],
      "settings": {
        "foreground": "#FA8072"
      }
    },

    {
      "name": "variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#FFA54F"
      }
    },
    {
      "name": "variables (JS)",
      "scope": [
        "variable.other.readwrite",
        "variable.other.dollar.js",
        "variable.other.object.dollar.js"
      ],
      "settings": {
        // "foreground": "#ff0000"
        "foreground": "#C0C0C0"
      }
    },

    {
      "name": "console",
      "scope": ["variable.other.object.js"],
      "settings": {
        "foreground": "#76EEC6"
      }
    },

    {
      "name": "invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#EE799F"
      }
    },
    {
      "name": "invalid.deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D2A8A1"
      }
    },

    {
      "name": "invalid.illegal",
      "scope": ["invalid.illegal"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EE799F"
      }
    }
  ]
}
